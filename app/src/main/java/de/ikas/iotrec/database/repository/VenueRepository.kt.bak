package de.ikas.iotrec.database.repository

import android.arch.lifecycle.LiveData
import android.support.annotation.WorkerThread
import de.ikas.iotrec.database.dao.VenueDao
import de.ikas.iotrec.database.model.Venue

class `VenueRepository.bak`(private val venueDao: VenueDao) {
    val allVenues: LiveData<List<Venue>> = venueDao.getAllVenues()

    @WorkerThread
    suspend fun insert(venue: Venue) {
        venueDao.insert(venue)
    }
}